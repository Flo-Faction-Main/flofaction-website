{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/shop", "destination": "/shop.html" },
    { "source": "/checkout", "destination": "/checkout.html" },
    { "source": "/services", "destination": "/services.html" },
    { "source": "/insurance", "destination": "/insurance.html" },
    { "source": "/quote", "destination": "/quote.html" },
    { "source": "/client-intake-form", "destination": "/client-intake-form.html" },
    { "source": "/voice-agent", "destination": "/index-voice-agent.html" },
    { "source": "/infinite-banking-calculator", "destination": "/infinite-banking-calculator.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
